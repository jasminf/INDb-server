doctype html
html
    head
        title= title
        link(rel='stylesheet' href='https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css' integrity='sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO' crossorigin='anonymous')
        link(href='/stylesheets/style_music.css' type='text/css' rel='stylesheet')
    body
        h1 Your search result:
        each searchResult in search
            h3=searchResult.name
            h3=searchResult.id
            img(src=searchResult.picture_xl)
            form(method='POST' action='/save/save_favorite')
                input(type="hidden" id="save" name="artistId" value=searchResult.id)
                button.btn.btn-primary(type='submit') save




